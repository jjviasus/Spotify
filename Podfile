# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Spotify' do
  use_frameworks!

  # dependency for image downloading
  pod 'SDWebImage'

  # allows us to show a rating prompt
  pod 'Appirater'

  # helps us track various things across the app
  pod 'Firebase/Analytics'

end
